var toolBar=function(e){this.direction="bottom",this.email="",this.type=e,this.createView(),this.setEmail(),this.setCancel(),this.setForm(),this.cencelEvent=!1};toolBar.prototype.getBtmView=function(){return this.view},toolBar.prototype.show=function(){this.view.bottom=0},toolBar.prototype.setProperty=function(e){this.property=e},toolBar.prototype.setCencelEvent=function(e){this.cencelEvent=e},toolBar.prototype.setCancel=function(){var e=this.view,t=this,i=Ti.UI.createButton({backgroundImage:"/funcIcon/cancel.png",width:50,height:50,style:"none",left:10});i.addEventListener("click",function(){e.bottom=-50,t.cencelEvent&&t.cencelEvent()}),this.view.add(i)},toolBar.prototype.setEmail=function(){var e=this,t=Ti.UI.createImageView({image:"/funcIcon/email.png",width:50,height:50,right:40});t.addEventListener("click",function(){var t=require("email").email;switch(e.type){case"property":t.sendProperty(e.property);break;default:t.sendProperty(e.property)}}),this.view.add(t)},toolBar.prototype.setForm=function(){var e=Ti.UI.createImageView({image:"/funcIcon/sign-up.png",width:50,height:50,right:120});this.view.add(e)},toolBar.prototype.createView=function(){var e=Ti.UI.createView({bottom:-50,height:50,width:"100%",backgroundColor:"#424242"});this.view=e},module.exports=toolBar;